<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="paperformat_print_waybill" model="report.paperformat">
            <field name="name">Waybill format</field>
            <field name="default" eval="False"/>
            <field name="format">A5</field>
            <field name="page_height">0</field>
            <field name="page_width">0</field>
            <field name="orientation">Portrait</field>
            <field name="margin_top">10</field>
            <field name="margin_bottom">10</field>
            <field name="margin_left">10</field>
            <field name="margin_right">10</field>
            <field name="header_spacing">30</field>
            <field name="dpi">96</field>
        </record>
        <record id="action_print_waybill" model="ir.actions.report">
            <field name="name">Print Waybill</field>
            <field name="model">giaohangnhanh.shipment</field>
            <field name="report_type">qweb-pdf</field>
            <field name="report_name">giaohangnhanh_connector.print_waybill</field>
            <field name="report_file">giaohangnhanh_connector.print_waybill</field>
<!--            <field name="binding_model_id" ref="model_giaohangnhanh_shipment"/>-->
            <field name="binding_type">report</field>
            <field name="attachment_use">False</field>
            <field name="paperformat_id" ref="paperformat_print_waybill"/>
        </record>
    </data>
</odoo>