<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <template id="header_footer_custom" inherit_id="web.internal_layout">
            <xpath expr="//div[hasclass('header')]" position="replace">
                <div class="header">
                </div>
            </xpath>
        </template>
        <template id="print_waybill_document" >
            <t t-call="giaohangnhanh_connector.header_footer_custom">
            </t>
        </template>
        <template id="print_waybill">
            <t t-call="web.html_container">
                <t t-call="giaohangnhanh_connector.print_waybill_document"/>
            </t>
        </template>
    </data>
</odoo>